extends layout

block content
  div F12를 눌러 console 탭과 network 탭을 확인하세요.
  script(src='/socket.io/socket.io.js')
  script.
    var socket = io.connect('http://localhost:8005', {
      path: '/socket.io',
      transports: ['websocket']
    });
    socket.on('news', function (data) {
      console.log(data);
      socket.emit('reply', 'Hello Node.JS');
    });
